timezone: UTC

schedule: {
  daily>: 07:00:00
}

+create_output: {
  sh>: touch output.yml
}

+get_variables: {
  _parallel: true
  +timezone: { sh>: "echo timezone: \\'${timezone}\\' >> output.yml" }
  +session_uuid: { sh>: "echo session_uuid: \\'${session_uuid}\\' >> output.yml" }
  +session_time: { sh>: "echo session_time: \\'${session_time}\\' >> output.yml" }
  +session_date: { sh>: "echo session_date: \\'${session_date}\\' >> output.yml" }
  +session_date_compact: { sh>: "echo session_date_compact: \\'${session_date_compact}\\' >> output.yml" }
  +session_local_time: { sh>: "echo session_local_time: \\'${session_local_time}\\' >> output.yml" }
  +session_tz_offset: { sh>: "echo session_tz_offset: \\'${session_tz_offset}\\' >> output.yml" }
  +session_unixtime: { sh>: "echo session_unixtime: \\'${session_unixtime}\\' >> output.yml" }
  +last_session_time: { sh>: "echo last_session_time: \\'${last_session_time}\\' >> output.yml" }
  +last_session_date: { sh>: "echo last_session_date: \\'${last_session_date}\\' >> output.yml" }
  +last_session_date_compact: { sh>: "echo last_session_date_compact: \\'${last_session_date_compact}\\' >> output.yml" }
  +last_session_local_time: { sh>: "echo last_session_local_time: \\'${last_session_local_time}\\' >> output.yml" }
  +last_session_tz_offset: { sh>: "echo last_session_tz_offset: \\'${last_session_tz_offset}\\' >> output.yml" }
  +last_session_unixtime: { sh>: "echo last_session_unixtime: \\'${last_session_unixtime}\\' >> output.yml" }
  +next_session_time: { sh>: "echo next_session_time: \\'${next_session_time}\\' >> output.yml" }
  +next_session_date: { sh>: "echo next_session_date: \\'${next_session_date}\\' >> output.yml" }
  +next_session_date_compact: { sh>: "echo next_session_date_compact: \\'${next_session_date_compact}\\' >> output.yml" }
  +next_session_local_time: { sh>: "echo next_session_local_time: \\'${next_session_local_time}\\' >> output.yml" }
  +next_session_tz_offset: { sh>: "echo next_session_tz_offset: \\'${next_session_tz_offset}\\' >> output.yml" }
  +next_session_unixtime: { sh>: "echo next_session_unixtime: \\'${next_session_unixtime}\\' >> output.yml" }
}