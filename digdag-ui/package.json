{
  "name": "digdag-ui",
  "version": "1.0.0",
  "description": "Digdag UI",
  "main": "console.jsx",
  "scripts": {
    "clean": "rimraf public",
    "build:js": "cross-env NODE_ENV=production webpack --config webpack.build.js -p --bail",
    "build:copy": "node prepare-release.js",
    "build": "yarn run clean && yarn run build:js && yarn run build:copy",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --hot --inline --progress --port 9000",
    "dev:https": "cross-env NODE_ENV=development webpack-dev-server --hot --inline --progress --port 9000 --https",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "lint": "standard",
    "test": "yarn run lint && yarn run flow"
  },
  "author": "",
  "license": "",
  "dependencies": {
    "babel-polyfill": "6.9.0",
    "bootstrap": "3.3.6",
    "brace": "^0.9.0",
    "buffer": "4.6.0",
    "duration": "^0.2.0",
    "jquery": "2.2.4",
    "js-untar": "0.1.4",
    "js-yaml": "3.6.1",
    "json-loader": "0.5.4",
    "lodash": "^4.15.0",
    "lru-cache": "4.0.1",
    "moment": "2.13.0",
    "node-uuid": "^1.4.7",
    "pako": "1.0.1",
    "path": "0.12.7",
    "react": "15.1.0",
    "react-addons-pure-render-mixin": "15.1.0",
    "react-dom": "15.1.0",
    "react-json-pretty": "1.2.0",
    "react-measure": "^1.3.1",
    "react-router": "2.4.1",
    "urijs": "1.18.1",
    "whatwg-fetch": "1.0.0"
  },
  "devDependencies": {
    "babel-eslint": "6.0.4",
    "babel-loader": "6.2.4",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-transform-class-properties": "6.9.0",
    "babel-plugin-transform-flow-strip-types": "6.8.0",
    "babel-preset-es2016": "6.0.11",
    "babel-preset-react": "6.5.0",
    "bootstrap-loader": "1.2.1",
    "bootstrap-sass": "3.3.6",
    "cross-env": "3.1.3",
    "css-loader": "0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "0.8.5",
    "flow-bin": "^0.32.0",
    "flow-status-webpack-plugin": "0.1.4",
    "html-webpack-plugin": "^2.22.0",
    "less": "2.7.1",
    "less-loader": "2.2.3",
    "node-fs-extra": "^0.8.1",
    "node-sass": "^3.10.0",
    "resolve-url-loader": "1.4.3",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.2",
    "standard": "^8.2.0",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.1",
    "webpack-dev-server": "1.14.1"
  },
  "standard": {
    "parser": "babel-eslint",
    "global": [
      "fetch",
      "DIGDAG_CONFIG"
    ]
  }
}
