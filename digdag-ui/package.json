    "jquery": "3.2.1",
    "js-untar": "0.1.4",
    "js-yaml": "3.8.2",
    "json-loader": "0.5.4",
    "lodash": "^4.15.0",
    "lru-cache": "4.0.2",
    "moment": "2.18.1",
    "moment-duration-format": "1.3.0",
    "node-uuid": "^1.4.7",
    "pako": "1.0.5",
