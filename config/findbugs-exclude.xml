<FindBugsFilter>
  <!-- generated class -->
  <Match>
    <Class name="~.*Immutable[^\.]+" />
    <Method name="~with.*" />
    <Bug pattern="ES_COMPARING_PARAMETER_STRING_WITH_EQ" />
  </Match>

  <!-- generated class -->
  <Match>
    <Class name="~.*Immutable[^\.]+" />
    <Method name="equals" />
    <Bug pattern="NP_METHOD_PARAMETER_TIGHTENS_ANNOTATION" />
  </Match>

  <!-- generated class -->
  <Match>
    <Class name="~.*Immutable[^\.]+" />
    <Method name="hashCode" />
    <Bug pattern="DMI_BLOCKING_METHODS_ON_URL" />
  </Match>

  <!-- generated class -->
  <Match>
    <Class name="~.*Immutable[^\.]+" />
    <Method name="equalTo" />
    <Bug pattern="DMI_BLOCKING_METHODS_ON_URL" />
  </Match>

  <!-- ignore: This is test code and no concern about SQL injection -->
  <Match>
    <Class name="utils.TemporaryDigdagServer" />
    <Method name="teardownDatabase" />
    <Bug pattern="SQL_NONCONSTANT_STRING_PASSED_TO_EXECUTE" />
  </Match>

  <!-- Should be investigated on CLI encoding behavior/specification -->
  <Match>
    <Or>
      <Class name="io.digdag.cli.GraphvizWorkflowVisualizer" />
      <Class name="io.digdag.cli.SelfUpdate"/>
      <Class name="io.digdag.cli.client.ClientCommand" />
      <Class name="io.digdag.cli.client.Delete"/>
      <Class name="io.digdag.cli.client.Secrets"/>
    </Or>
    <Bug pattern="DM_DEFAULT_ENCODING" />
  </Match>

  <!-- Should be fixed -->
  <Match>
    <Class name="io.digdag.core.workflow.TaskTree" />
    <Bug pattern="DM_BOXED_PRIMITIVE_FOR_COMPARE" />
  </Match>
</FindBugsFilter>
